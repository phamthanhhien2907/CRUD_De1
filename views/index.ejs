<!DOCTYPE html>
<html>
  <head>
    <title>Danh SÃ¡ch user</title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1>Danh Sach user</h1>
    <a href="form-add"><button>Add user</button></a><br /><br />
    <table border="2">
      <tr>
        <th>ID</th>
        <th>IdDistrict</th>
        <th>IdVillage</th>
        <th>Address</th>
        <th>IsGetNotification</th>
        <th>IsVerified</th>
        <th>IsGetOpen</th>
        <th>ACTION</th>
      </tr>
      <% user3s.forEach(item => { %>
      <tr>
        <td><%= item._id %></td>
        <td><%= item.IdDistrict %></td>
        <td><%= item.IdVillage %></td>
        <td><%= item.Address %></td>
        <td><%=item.IsGetNotification %></td>
        <td><%=item.IsVerified %></td>
        <td><%=item.IsGetOpen %></td>
        <td>
          <a href="/form-update/<%= item._id%>">Update</a>||<a
            href="/form-delete/<%= item._id%>"
            >Delete</a
          >
        </td>
      </tr>
      <% }) %>
    </table>
  </body>
</html>
